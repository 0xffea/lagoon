node {

  stage ('Checkout') {
    checkout scm
  }

  ansiColor('xterm') {
    stage ('run test') {
      try {
        sh "make end2end-tests -j4"
      } catch (e) {
        echo "Something went wrong, trying to cleanup"
        sh "make end2end-tests/clean"
        throw e
      }
      sh "make end2end-tests/clean"
    }
  }

}